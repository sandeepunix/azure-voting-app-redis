pipeline {
    agent any

    stages {
        stage('Verify Branch') {
            steps {
                echo ${GIT_BRANCH}
            }
        }
        stage('lala from panna') {
            steps {
                echo 'lala from panna'
            }
        }
        stage('powershell') {
            steps {
                 powershell 'Write-Output "Hello World!"'
            }
        }
    }
}
